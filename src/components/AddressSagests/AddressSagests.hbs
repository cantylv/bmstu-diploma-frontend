<div class="address-sagests">
	<div class="address-sagests__search-container">
		<div class="address-sagests__input-container">
			<img class="address-sagests__search-icon" src="assets/search-icon.svg" alt="search" />
			<input
				type="text"
				value="{{this.value}}"
				placeholder="Введите улицу или дом"
				id="address-search-input"
			/>
			<img class="address-sagests__clear-icon" src="assets/close.svg" alt="close" />
		</div>
	</div>

	<div class="address-sagests__dropdown-container"></div>
</div>
